{
  "kodi": {
    "address": "127.0.0.1",
    "port": 9090
  },
  "debug": true,
  "callbacks": {
    "Player.OnPlay": [
      {
        "types": ["movie"],
        "backend": "shell",
        "command": "/usr/bin/curl",
        "arguments": ["http://127.0.0.1:3000/karaoke/onplay"]
      }
    ],
    "Player.OnStop": [
      {
        "types": ["movie"],
        "backend": "shell",
        "command": "/usr/bin/curl",
        "arguments": ["http://127.0.0.1:3000/karaoke/onstop"]
      }
    ]
  }
}

